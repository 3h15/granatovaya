      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {"source": "/toutes-nos-actualites.html", "function": "newsIndex"},
      {"source": "/actualites/**", "function": "news"}
    ]
  },
  "database": {
    "rules": "database.bolt"
  }
}

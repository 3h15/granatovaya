        var siege = {lat: 48.7321012, lng: 2.7276595};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 10,
          center: siege,
          scrollwheel: false,
          styles: [
    {
        "featureType": "all",
        "elementType": "all",
        "stylers": [
            {
                "visibility": "on"
            },
            {
                "hue": "#006699"
            },
            {
                "saturation": -18
            },
            {
                "lightness": 47
            }
        ]
    },
    {
        "featureType": "poi",
        "elementType": "all",
        "stylers": [
            {
                "visibility": "off"
            }
        ]
    },
    {
